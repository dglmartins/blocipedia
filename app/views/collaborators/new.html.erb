<h1>Select a collaborator</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for [@wiki, @collaborator] do |f| %>
      <div class="form-group">
        
          <%= f.collection_select(:user, User.all, :id, :email) %>

     </div>
     <div class="form-group">
       <%= f.submit "Save" %>
     </div>
    <% end %>
  </div>

</div>
